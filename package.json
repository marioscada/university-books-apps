    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.46.4"
  },
  "description": "University Books Mobile - Angular Ionic App with AWS Cognito Authentication",
