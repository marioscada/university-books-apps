{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "university-books-mobile": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/university-books-mobile",
      "sourceRoot": "projects/university-books-mobile/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/university-books-mobile/src/styles"
              ]
            },
            "outputPath": "dist/university-books-mobile",
            "index": "projects/university-books-mobile/src/index.html",
            "browser": "projects/university-books-mobile/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/university-books-mobile/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/university-books-mobile/public"
              }
            ],
            "styles": [
              "projects/university-books-mobile/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "projects/university-books-mobile/src/environments/environment.ts",
                  "with": "projects/university-books-mobile/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "university-books-mobile:build:production"
            },
            "development": {
              "buildTarget": "university-books-mobile:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/university-books-mobile/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/university-books-mobile/public"
              }
            ],
            "styles": [
              "projects/university-books-mobile/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/university-books-mobile/src/**/*.ts",
              "projects/university-books-mobile/src/**/*.html"
            ]
          }
        }
      }
    },
    "backend-api-client": {
      "projectType": "library",
      "root": "libs/backend-api-client",
      "sourceRoot": "libs/backend-api-client/src",
      "prefix": "ub",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "libs/backend-api-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/backend-api-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/backend-api-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "libs/backend-api-client/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "libs/backend-api-client/**/*.ts",
              "libs/backend-api-client/**/*.html"
            ],
            "eslintConfig": "libs/backend-api-client/eslint.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "angular-eslint"
    ]
  }
}
